//消息 接口
// params: uid, 用户id
// gameid, 游戏id
// gamename, 游戏名称
// timestamp 当前时间
// mid: 聊天记录id， 为空时返回当前时间timestamp往前的20条消息
{
  "State": 1,
  "Descriptor": "数据获取成功",
  "Version": "2019-03-31 20:50:52",
  "Data": {
  "current": {
      "process": 1,          //2--封盘开奖中，1--倒计时
      "countdown": 166,      //倒计时时间，process为0时此值为-1
      "fptime": 52353646664, //下次封盘时间（封盘时间和开奖时间之间 的间隔应该是固定的，5秒或者10秒，这里只是个例子）
      "kjtime": 42655235235, //下次开奖时间，封盘中则为空
      "serial": "2402138",   //期数
      "people": 1124,        //在线人数,
       "money": "0.00",      //账户余额
      "history": [{
        "serial": "2402137",
          "time": "03-14 21:32",
         "value": "5 + 6 + 3=14 ( \u5927\u53cc )"
        },
        {
          "serial": "2402136",
            "time": "03-14 21:28",
           "value": "1 + 6 + 3=10 ( \u5c0f\u53cc )"
        },
        {
          "serial": "2402135",
            "time": "03-14 21:25",
           "value": "7 + 1 + 9=17 ( \u5927\u5355 )"
        },
        {
          "serial": "2402134",
          "time": "03-14 21:21",
          "value": "5 + 1 + 4=10 ( \u5c0f\u53cc )"
        },
        {
          "serial": "2402133",
          "time": "03-14 21:18",
          "value": "8 + 2 + 9=19 ( \u5927\u5355 )"
        },
        {
          "serial": "2402132",
          "time": "03-14 21:14",
          "value": "2 + 7 + 2=11 ( \u5c0f\u5355 )"
        },
        {
          "serial": "2402131",
          "time": "03-14 21:11",
          "value": "6 + 3 + 8=17 ( \u5927\u5355 )"
        },
        {
          "serial": "2402130",
          "time": "03-14 21:07",
          "value": "2 + 4 + 1=7 ( \u5c0f\u5355 )"
        },
        {
          "serial": "2402129",
          "time": "03-14 21:04",
          "value": "6 + 1 + 3=10 ( \u5c0f\u53cc )"
        },
        {
          "serial": "2402128",
          "time": "03-14 21:00",
          "value": "4 + 0 + 6=10 ( \u5c0f\u53cc )"
        }
      ]
    },

    "message": [  消息id  timestamp， type，content（json）
      //system_kj--开奖消息，(需要定时器插入开奖消息)
      //system_xz--下注消息, (下注的时候往消息表里面插入的)
      //system_fp--封盘消息,（定时器来触发）
      //user_enter--用户进入房间消息,（用户进入房间请求接口）
      //user_text--用户文本消息，（聊天的文本框）
      {
          "type": "system_kj",
          "content": {
          "serial": "2402137", //期数
          "time": "03-14 21:32", //时间
          "value": "5 + 6 + 3=14 ( \u5927\u53cc )" //开奖结果
            }
      },
      {
          "type": "system_xz",
          "content": {
          "serial": "2402137", //期数
          "time": "03-14 21:32", //时间
          "uid": "23124", //用户id
          "uname": "安妮牛**", //用户名
          "total": 140, //合计
          "items": [
            {
              "type": "小",
              "money": "70"
            },
            {
              "type": "大",
              "money": "70"
            }
          ]
        }
      },
      {
        "type": "system_fp",
        "content": {
        //这里有个下注汇总，我这里没截到图，你按照账单的情况设计一下这个结构
        }
      },
      {
        "type": "user_enter",
        "content": {
          "uname": "安妮牛**" //用户名
        }
      },
      {
        "type": "user_text", //system_kj--开奖消息， system_xz--下注消息, system_fp--封盘消息, user_enter--用户进入房间消息, user_text--用户文本消息
        "content": {
          "uname":"安妮牛**",
          "text":"70大70小"
        }
      }
    ]
  }
}

